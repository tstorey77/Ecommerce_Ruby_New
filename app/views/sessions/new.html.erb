<div class="container">
   <div class="notification">
      <h1 class="subtitle">Login</h1>
      <%= form_tag '/login' do %>
         <%= label_tag :username%>
         <%= text_field_tag :username %>
         <%= label_tag :password%>
         <%= password_field_tag :password%>
         <br><br>
         <%= submit_tag "Login"%>
      <%end%>
   </div>
 </div>


